    "test:watch": "npm test -- --watch",
    "debug": "node-debug _mocha --compilers js:babel-core/register --recursive"
  },
  "author": "",
  "license": "BSD-2-Clause-FreeBSD",
  "dependencies": {
    "@graphene/ecc": "^1.0.0",
    "@graphene/time-token": "^1.0.0",
