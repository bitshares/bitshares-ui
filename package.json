{
    "name": "BitShares2-light",
    "version": "5.0.220214",
    "description": "Reference UI to showcase all features of the BitShares blockchain.",
    "homepage": "https://github.com/bitshares/bitshares-ui",
    "author": "BitShares UI Worker <ui-dev@bitshares.org>",
    "contributors": [
        "Sigve Kvalsvik <kvalsvik.crypto@gmail.com>",
        "James Calfee",
        "Dan Larimer",
        "Valentine Zavgorodnev <valentine.zavgorodnev@gmail.com>"
    ],
    "license": "MIT",
    "engines": {
        "node": ">=16.x",
        "yarn": ">=1.0"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/bitshares/bitshares-ui"
    },
    "main": "build/electron/index.js",
    "scripts": {
        "postinstall": "yarn run install-charting-library",
        "compile-tv-css": "sass ./charting_library/scss/:./charting_library/ --style compressed",
        "install-charting-library": "node ./charting_library/install-charting-library.cjs && yarn run compile-tv-css",
        "test:market": "cross-env NODE_ENV=test mocha --require @babel/register --watch --watch-extensions js ./app/test/marketTests",
        "bench:market": "cross-env NODE_ENV=test babel ./lib/common/MarketClasses.js -o ./app/test/MarketClasses.js && cross-env NODE_ENV=test node --harmony ./test/marketBenchmark",
        "profile-dev": "webpack --env dev --env profile --profile --json > stats-dev.json",
        "profile-build": "cross-env NODE_ENV=production webpack --env prod --env profile --env hash --env baseUrl=false --profile --json > stats-prod.json",
        "profile-nougly": "cross-env NODE_ENV=production webpack --env prod --env profile --env hash --env noUgly --profile --json > stats-prod-nougly.json",
        "start": "cross-env NODE_ENV=development webpack serve",
        "start-perf": "cross-env NODE_ENV=development webpack serve perf-dev",
        "prestart-electron": "cross-env NODE_ENV=production webpack --env hash --env noUgly --env electron --env prod && yarn run prepare-electron",
        "start-electron": "electron build/electron/index.js",
        "fix-locale-files": "node ./app/assets/locales-fixup.js",
        "create-notification-string": "node ./app/assets/create-notifcation-string.js",
        "remove-english-locale-duplicates": "node ./app/assets/locales-remove-english-duplicates.js",
        "postfix-locale-files": "./node_modules/prettier/bin-prettier.js app/assets/locales/locale-*.json --write",
        "build": "cross-env NODE_ENV=production webpack --env prod",
        "build-nougly": "cross-env NODE_ENV=production webpack --env prod --env noUgly",
        "build-hash": "cross-env NODE_ENV=production webpack --env prod --env hash --env baseUrl=false",
        "build-hash-nougly": "cross-env NODE_ENV=production webpack --env prod --env hash --env noUgly --env baseUrl=false",
        "build-github": "cross-env NODE_ENV=production webpack --env prod --env hash --env.deprecated --env baseUrl='/wallet/'",
        "build-electron": "cross-env NODE_ENV=production webpack --env prod --env electron --env hash",
        "build-electron-nougly": "cross-env NODE_ENV=production webpack --env prod --env electron --env noUgly --env hash --progress --display-error-details --colors",
        "prepare-electron": "copyfiles -f resources/*.js build/electron/",
        "postbuild-electron": "yarn run prepare-electron",
        "postbuild-electron-nougly": "yarn run prepare-electron",
        "build-win32": "cross-env NODE_ENV=production && webpack --env prod --progress --colors &&  echo 'copying to electron/build..' &&  xcopy dist ..\\electron\\build /s /e /y && echo 'done.'",
        "precommit": "pretty-quick --staged",
        "prepackage": "yarn run build-electron",
        "prepackage-no-ugly": "yarn run build-electron-nougly",
        "package": "electron-builder -p never",
        "package-charting-library": "node ./charting_library/package-charting-library.js",
        "package-no-ugly": "electron-builder -p never",
        "package-deb": "yarn run prepackage && electron-builder  -p never --linux deb",
        "package-snap": "yarn run prepackage && electron-builder  -p never --linux snap",
        "package-win": "yarn run prepackage && electron-builder  -p never --windows",
        "package-mac": "yarn run prepackage && electron-builder  -p never --mac dmg",
        "postpackage": "node write_digest.js",
        "scam": "node scamParser.js"
    },
    "jest": {
        "scriptPreprocessor": "<rootDir>/__tests__/jest-preprocessor.js",
        "testPathDirs": [
            "<rootDir>/__tests__"
        ],
        "unmockedModulePathPatterns": [
            "<rootDir>/node_modules/react",
            "<rootDir>/node_modules/tcomb",
            "<rootDir>/node_modules/immutable"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json",
            "coffee"
        ],
        "testFileExtensions": [
            "js",
            "jsx"
        ],
        "testPathIgnorePatterns": [
            "/jest-preprocessor.js",
            "/node_modules/",
            "/utils/"
        ]
    },
    "browsersList": "electron 1.7",
    "build": {
        "productName": "BitShares",
        "appId": "org.bitshares.graphene",
        "files": [
            "build/electron",
            "!node_modules"
        ],
        "mac": {
            "category": "public.app-category.finance",
            "artifactName": "${productName}_${version}.${ext}"
        },
        "dmg": {
            "artifactName": "${productName}_${version}.${ext}",
            "contents": [
                {
                    "x": 130,
                    "y": 220
                },
                {
                    "x": 410,
                    "y": 220,
                    "type": "link",
                    "path": "/Applications"
                }
            ]
        },
        "win": {
            "artifactName": "${productName}_${version}.${ext}",
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        },
        "linux": {
            "artifactName": "${productName}_${version}.${ext}",
            "category": "Finance",
            "target": [
                "deb",
                "AppImage",
                "snap"
            ],
            "synopsis": "BitShares: Cryptocurrency and Decentralized Exchange"
        },
        "directories": {
            "buildResources": "resources",
            "output": "build/binaries"
        }
    },
    "dependencies": {
        "alt": "https://github.com/bitshares/alt.git",
